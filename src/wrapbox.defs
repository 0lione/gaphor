)

(define-object HWrapBox
  (in-module "Gtk")
  (parent "GtkWrapBox")
  (c-name "GtkHWrapBox")
  (gtype-id "GTK_TYPE_HWRAP_BOX")
)

;; Enumerations and flags ...


;; From gtkhwrapbox.h

(define-function gtk_hwrap_box_get_type
