;; -*- scheme -*-
; object definitions ...
(define-object WrapBox
  (in-module "Gtk")
  (parent "GtkContainer")
  (c-name "GtkWrapBox")
  (gtype-id "GTK_TYPE_WRAP_BOX")
)

(define-object VWrapBox
  (in-module "Gtk")
