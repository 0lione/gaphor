<Root>

    <menu>
	<submenu name="File" _label="_File">
	    <menuitem name="FileExportSVG" verb=""/>
	    <separator/>
	    <menuitem name="FileClose" verb=""/>
	</submenu>

	<submenu name="Edit" _label="_Edit">
	    <menuitem name="EditUndo" verb=""/>
	    <menuitem name="EditRedo" verb=""/>
	    <separator/>
	    <menuitem name="EditDelete" verb=""/>
	    <separator/>
	    <menuitem name="EditSelectAll" verb=""/>
	    <menuitem name="EditUnselectAll" verb=""/>
	    <separator/>
	    <placeholder name="EditOps"/>
	    <separator/>
	    <menuitem name="EditProperties" verb="" _label="_Properties..."/>
	</submenu>

	<submenu name="View" _label="_View">
	    <menuitem name="ViewZoomIn" verb=""/>
	    <menuitem name="ViewZoomOut" verb=""/>
	    <menuitem name="ViewZoom100" verb=""/>
	    <separator/>
	    <menuitem name="SnapToGrid" verb="" type="toggle"/>
	</submenu>

	<submenu name="Insert" _label="_Insert">
	    <menuitem name="InsertActor" verb=""/>
	    <menuitem name="InsertUseCase" verb=""/>
	    <menuitem name="InsertClass" verb=""/>
	    <menuitem name="InsertPackage" verb=""/>
	    <menuitem name="InsertComment" verb=""/>
	    <menuitem name="InsertCommentLine" verb=""/>
	    <menuitem name="InsertAssociation" verb=""/>
	    <menuitem name="InsertDependency" verb=""/>
	    <menuitem name="InsertGeneralization" verb=""/>
	</submenu>
    </menu>

    <dockitem name="Toolbar" _tip="Diagram toolbar" behavior="detachable,exclusive">
	<toolitem name="InsertActor" verb=""/>
	<toolitem name="InsertUseCase" verb=""/>
	<separator/>
	<toolitem name="InsertClass" verb=""/>
	<toolitem name="InsertPackage" verb=""/>
	<toolitem name="InsertAssociation" verb=""/>
	<toolitem name="InsertDependency" verb=""/>
	<toolitem name="InsertGeneralization" verb=""/>
	<separator/>
	<toolitem name="InsertComment" verb=""/>
	<toolitem name="InsertCommentLine" verb=""/>
    </dockitem>

    <popups>
	<popup name="DiagramView">
	    <menuitem name="EditDelete" verb=""/>
	    <menuitem name="SnapToGrid" verb="" type="toggle"/>
	    <separator/>
	    <menuitem name="CreateAttribute" verb=""/>
	    <menuitem name="CreateOperation" verb=""/>
	</popup>
    </popups>

    <status>
	<item name="main"/>
    </status>

</Root>
