<submenu name="Insert" _label="_Insert">

        <menuitem name="InsertDiagram" verb=""/>
        <menuitem name="InsertToggle" verb=""/>

</submenu>
	
<submenu name="Settings" _label="_Settings">

	<menuitem name="BonoboCustomize" verb="" _label="Customi_ze"/>

	<separator/>
        <menuitem name="SettingsPreferences" verb="" _label="_Preferences"/>

</submenu>

<submenu name="Window" _label="_Window">
<!--
        <menuitem name="WindowCreateNew" verb="" _label="Create New _Window"/>
-->
	<placeholder name="OpenWindows"/>

        <menuitem name="WindowClose" verb="" _label="_Close This Window"/>

</submenu>

<placeholder name="Component"/>

<submenu name="Help" _label="_Help">
	
	<placeholder name="BuiltMenuItems"/>

        <menuitem name="About" verb=""/>

</submenu>

<submenu name="Debug" _label="_Debug" hidden="0">

	<menuitem name="DebugDumpXml" verb=""/>

</submenu>

</menu>

<dockitem name="Toolbar" _tip="Main toolbar" behavior="detachable,exclusive">
	<toolitem name="FileNew" verb=""/>
	<toolitem name="FileOpen" verb=""/>
	<toolitem name="FileSave" verb=""/>
	<toolitem name="FileSaveAs" verb=""/>

	<separator/>

</dockitem>

<status>
	<item name="main"/>
</status>

</Root>
